<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:carouselBugRepro="clr-namespace:carousel_bug_repro"
             x:Class="carousel_bug_repro.TestItemView"
             x:DataType="carouselBugRepro:TestItem">
    <VerticalStackLayout>
        <Label Text="{Binding Title}" FontSize="20"/>
        <Label Text="{Binding HtmlText}" TextType="Html" FontSize="20"/>
    </VerticalStackLayout>
</ContentView>